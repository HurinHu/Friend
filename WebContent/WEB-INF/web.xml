<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>FYPC</display-name>
  <welcome-file-list>
    <welcome-file>index</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>index</servlet-name>
    <servlet-class>servlet.Index</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>servlet.login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>assessment</servlet-name>
    <servlet-class>servlet.Assessment</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>change</servlet-name>
    <servlet-class>servlet.changepw</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ajax</servlet-name>
    <servlet-class>servlet.Ajax</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>user</servlet-name>
    <servlet-class>servlet.User</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>import</servlet-name>
    <servlet-class>servlet.Import</servlet-class>
    <init-param>
      <param-name>savePath</param-name>
      <param-value>upload</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>timetable</servlet-name>
    <servlet-class>servlet.TimeTable</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>studentlist</servlet-name>
    <servlet-class>servlet.Studentlist</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>gradelist</servlet-name>
    <servlet-class>servlet.Gradelist</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>assessment</servlet-name>
    <url-pattern>/assessment</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>change</servlet-name>
    <url-pattern>/change</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ajax</servlet-name>
    <url-pattern>/ajax</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>user</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>import</servlet-name>
    <url-pattern>/import</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>timetable</servlet-name>
    <url-pattern>/timetable</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>studentlist</servlet-name>
    <url-pattern>/studentlist</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>gradelist</servlet-name>
    <url-pattern>/gradelist</url-pattern>
  </servlet-mapping>
</web-app>